<%= form_for @songs do |f| %>
  <%= render "shared/errors", object: @songs %>
  <%= f.text_field :field_1, class: "form-control" %>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
